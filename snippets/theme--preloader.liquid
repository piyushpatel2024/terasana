<style>
  /* Must be here and not in the styles/ folder */
  
  #PreLoader {
    background: var(--brand-color-dark-alt) !important;
    z-index: 9999;
    width: 100%;
    overflow: hidden;
    position: absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    opacity: 1;
    /* transition: .1s; */
  }
  
  #logo {
    display: flex;
    justify-content: center;
    align-items: center;
    -webkit-text-fill-color: white;
    height: 100vh;
    z-index: 1001;
    fill: #fff
  }

  #logo svg {
    z-index: 10;
  }   

  html.swup-enabled #PreLoader {
    transition: .3s;
    opacity: 0;
    z-index: -3;
    width: 0;
    height: 0;
  }    
</style>


  