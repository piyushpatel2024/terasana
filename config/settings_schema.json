  {
    "name": "Discounts & Shipping",
    "settings": [
      {
        "type": "checkbox",
        "id": "cart_savings_enabled",
        "label": "Display total savings bar if applicable.",
        "default": true
      },
      {
        "type": "header",
        "content": "Automatic Cart Discounts"
      },
      {
        "type": "checkbox",
        "id": "cart_automatic_discounts_enabled",
        "label": "Enable automatic discounts",
        "info": "Enabled the whole module. Individual settings bellow",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "cart_free_shipping_enabled",
        "label": "Enable and display amount to free shipping discount",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "cart_first_percent_off_enabled",
        "label": "Enable 1st discount option",
        "info": "1st discount options configured bellow",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "cart_second_percent_off_enabled",
        "label": "Enable 2nd discount option",
        "info": "2nd discount options configured bellow",
        "default": true
      },
      {
        "type": "text",
        "id": "cart_free_shipping_name",
        "default": "Free Shipping",
        "label": "Free shipping name",
        "info": "Shipping rule name displayed in the cart"
      },
      {
        "type": "text",
        "id": "cart_free_shipping_earned",
        "default": "Congrats, you've unlocked free shipping!",
        "label": "Free shipping earned msg",
        "info": "Message to show on the notification when the order meets the shipping discount"
      },
      {
        "type": "text",
        "id": "cart_free_shipping_not_earned",
        "default": "to unlock free shipping!",
        "label": "Free shipping not earned message",
        "info": "Message to show on the notification when the order doesn't yet meet the shipping discount"
      },
      {
        "type": "text",
        "id": "cart_free_shipping_threshold",
        "default": "30",
        "label": "Order total threshold to get free shipping option. ie. 30",
        "info": "Must match a shipping rule defined in the shipping settings"
      },
      {
        "type": "select",
        "id": "cart_free_shipping_type",
        "options": [
          {
            "value": "percent",
            "label": "Percent"
          },
          {
            "value": "amount",
            "label": "Amount"
          }
        ],
        "default": "amount",
        "label": "Discount type (saving)"
      },
      {
        "type": "text",
        "id": "cart_free_shipping_amount",
        "default": "5",
        "label": "Amount or Percent saved by this discount",
        "info": "Must match the type defined above. i.e 5 for (5%) or 5 for $5"
      },
      {
        "type": "text",
        "id": "cart_first_discount_code",
        "default": "10POFF50",
        "label": "1st discount code",
        "info": "Must match an active discount code"
      },
      {
        "type": "text",
        "id": "cart_first_discount_threshold",
        "default": "50",
        "label": "$ amount that triggers the discount",
        "info": "Cart total amount that triggers the 1st discount"
      },
      {
        "type": "select",
        "id": "cart_first_discount_type",
        "options": [
          {
            "value": "percent",
            "label": "Percent"
          },
          {
            "value": "amount",
            "label": "Amount"
          }
        ],
        "default": "percent",
        "label": "Discount type (saving)"
      },
      {
        "type": "text",
        "id": "cart_first_discount_amount",
        "default": "10",
        "label": "Amount or Percent saved by this discount",
        "info": "Must match the type defined above. i.e 10 for (10%) or 10 for $10"
      },
      {
        "type": "text",
        "id": "cart_first_discount_name",
        "default": "10% Off",
        "label": "1st discount name",
        "info": "Discount name displayed in the cart"
      },
      {
        "type": "text",
        "id": "cart_second_discount_code",
        "default": "20POFF80",
        "label": "2nd discount code",
        "info": "Must match an active discount code i.e 20POFF80"
      },
      {
        "type": "text",
        "id": "cart_second_discount_threshold",
        "label": "$ amount that triggers the discount",
        "default": "80",
        "info": "Cart total amount that triggers the 2nd discount"
      },
      {
        "type": "select",
        "id": "cart_second_discount_type",
        "options": [
          {
            "value": "percent",
            "label": "Percent"
          },
          {
            "value": "amount",
            "label": "Amount"
          }
        ],
        "default": "percent",
        "label": "Discount type (saving) "
      },
      {
        "type": "text",
        "id": "cart_second_discount_amount",
        "default": "20",
        "label": "Amount or Percent saved by this discount",
        "info": "Must match the type defined above. i.e 10 for (20%) or 20 for $20"
      },
      {
        "type": "text",
        "id": "cart_second_discount_name",
        "default": "20% Off",
        "label": "2nd discount name",
        "info": "Discount name displayed in the cart "
      }
    ]
