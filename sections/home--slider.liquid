{% capture blocksList %}{% include 'data-json--slider' %}{% endcapture %}

<section
  data-section-id="{{ section.id }}"
  data-section-type="index-slider"
  data-vue-section-id="{{ section.id }}"
  data-vue-list="{{ blocksList | url_escape }}"
  data-vue-theme="{{ section.section_theme | json | url_escape }}"
  data-vue-settings="{{ section.settings | json | url_escape }}">
  {%comment%}<div class="page-width db pa2 bg-yellow">{{ section.settings | json}}</div>{%endcomment%}
  {% comment %} <div class="page-width db pa2 bg-orange">{{ blocksList }}</div> {% endcomment %}
  <div id="vue-index-slider"></div>
</section>

